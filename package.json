{
  "name": "om-file-orchestrator",
  "version": "1.0.0",
  "description": "A powerful Node.js library for automatically downloading and managing weather data files from OpenMeteo's spatial data API",
  "main": "lib/index.js",
  "bin": {
    "om-orchestrator": "./index.js"
  },
  "scripts": {
    "test": "node test/test-scenarios.js",
    "test:permissions": "node test/test-permissions.js",
    "test:priority": "node test/test-priority.js",
    "test:quick": "node test/test-permissions.js",
    "start": "node index.js",
    "start:fake": "node index.js --fake",
    "dev": "node index.js --immediate",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "example:simple": "node examples/simple-usage.js",
    "example:fastify": "node examples/fastify-integration.js",
    "example:custom": "node examples/custom-config.js"
  },
  "author": "Sebastien Etienne <thespotsguru@gmail.com>",
  "license": "ISC",
  "keywords": [
    "weather",
    "meteorology",
    "openmeteo",
    "data-download",
    "orchestrator",
    "automation",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SebSwan/om-file-orchestrator.git"
  },
  "bugs": {
    "url": "https://github.com/SebSwan/om-file-orchestrator/issues"
  },
  "homepage": "https://github.com/SebSwan/om-file-orchestrator#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "fs-extra": "^11.3.1",
    "node-cron": "^4.2.1",
    "p-queue": "^8.1.1",
    "winston": "^3.17.0"
  }
}
